    <validationRules>
        <fullName>ReqRIOMFields</fullName>
        <active>true</active>
        <description>Enforces completion of related fields based on RIOM type.</description>
        <errorConditionFormula>OR(
AND
(ISPICKVAL(RIOM_Type__c,&quot;Risk&quot;),
OR(ISBLANK (TEXT (Risk_Likelihood__c)), 
ISBLANK (TEXT (Risk_Status__c)))),
AND (
ISPICKVAL (RIOM_Type__c, &quot;Issue&quot;), 
OR(ISBLANK (Issue_Detail__c), 
ISBLANK (TEXT (Issue_Status__c)))),
AND (
ISPICKVAL (RIOM_Type__c, &quot;Opportunity&quot;), 
OR(ISBLANK (TEXT (Opportunity_Area__c)), 
ISBLANK (TEXT (Opportunity_Factor__c)))),
AND (
ISPICKVAL(RIOM_Type__c,&quot;Risk&quot;),
OR(NOT (ISBLANK (Issue_Detail__c)), 
NOT (ISBLANK (TEXT (Issue_Status__c))),
NOT(ISBLANK (TEXT (Opportunity_Area__c))), 
NOT(ISBLANK (TEXT (Opportunity_Factor__c))))),
AND (
ISPICKVAL(RIOM_Type__c,&quot;Issue&quot;),
OR(NOT (ISBLANK (TEXT(Risk_Likelihood__c))), 
NOT (ISBLANK (TEXT (Risk_Status__c))),
NOT(ISBLANK (TEXT (Opportunity_Area__c))), 
NOT(ISBLANK (TEXT (Opportunity_Factor__c))))),
AND (
ISPICKVAL(RIOM_Type__c,&quot;Opportunity&quot;),
OR(NOT (ISBLANK (TEXT(Risk_Likelihood__c))), 
NOT (ISBLANK (TEXT (Risk_Status__c))),
NOT(ISBLANK (Issue_Detail__c)), 
NOT(ISBLANK (TEXT (Issue_Status__c))))))</errorConditionFormula>
        <errorMessage>*Complete the related RIOM fields for the RIOM Type selected*
  RISK requires Risk Likelihood and Risk Status
   ISSUE requires Issue Detail and Issue Status
   OPPORTUNITY requires Opportunity Area and Opportunity Factor</errorMessage>
    </validationRules>
